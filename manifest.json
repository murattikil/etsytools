{
    "background": {
        "scripts": ["jquery.js", "background.js"]
    },
    "page_action": {
        "default_icon": {
            "19": "img/icon19.png",
            "38": "img/icon38.png"
        },
        "default_title": "EtsyTools"
    },
    "content_scripts": [{
        "css": ["styles.css"],
        "js": ["jquery.js", "content.js"],
        "matches": ["*://*.etsy.com/*"],
        "run_at": "document_end"
    }],
    "web_accessible_resources": ["audio/*"],
    "description": "It's tools for Etsy.com",
    "homepage_url": "http://etsytools.reatlat.net/",
    "icons": {
        "128": "img/icon128.png",
        "16": "img/icon16.png",
        "48": "img/icon48.png"
    },
    "manifest_version": 2,
    "name": "EtsyTools",
    "key": "",
    "permissions": ["webRequest", "storage", "tabs", "*://*.etsy.com/"],
    "short_name": "EtsyTools",
    "version": "1.4.4"
}
